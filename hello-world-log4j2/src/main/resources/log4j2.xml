<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="INFO">
    <Appenders>
        <Console name="JSON" target="SYSTEM_OUT">
            <JsonLayout compact="true" eventEOL="true" includeTimeMillis="true"></JsonLayout>
        </Console>
    </Appenders>
    <Loggers>
        <Root>
            <AppenderRef ref="JSON"/>
        </Root>
    </Loggers>
</Configuration>
